  </div>

  <div class="row index-page__section">
    <div class="card-list">
      <% current_page.data.primary_cards.each do |study| %>
        <div class="study-card">
          <div style="position:relative;height:280px">
            <img
            src="<%=study.image.url%>"
            height="<%=study.image.height || "260px"%>"
            />
          </div>
          <div class="study-blurb">
            <h2><%=study.name%></h2>
            <p><%=study.blurb%></p>
            <a href="#" class="btn btn-big btn-primary card__sticky-button" >
              Read More
            </a>
          </div>
        </div>
      <% end %>
    </div>
  </div>

  <div class="row index-page__section">
    <h1> Start building with tradegecko api in minutes...</h1>
  </div>

  <div class="row">
    <div class="card-list">
